    "formik": "^2.1.4",
    "lodash.isequal": "^4.5.0",
    "qrcode.react": "^1.0.0",
    "react": "^16.13.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.0",
    "react-typist": "^2.0.5",
    "secure-random": "^1.1.2",
    "speakingurl": "^14.0.1",
    "webext-base-css": "^1.0.0",
    "webextension-polyfill-ts": "^0.13.0"
  },
